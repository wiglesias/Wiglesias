{% if object.imageName is not null %}
    <img src="{{ vich_uploader_asset(object, 'imageFile') | imagine_filter(filter) }}" class="img-responsive" alt="{{ object.imageName }}">
{% else %}
    <img src="{{ asset('bundles/app/img/default-post.jpg') | imagine_filter(filter) }}" class="img-responsive" alt="default image">
{% endif %}